{
  "ignorePatterns": [
    { "pattern": "^http://homeding/" },
    { "pattern": "^http://airding/" },
    { "pattern": "^http://outdoor/" },
    { "pattern": "/_.*.md$" },
    { "pattern": "^http://ESP-xxxxxx/" },
    { "pattern": "^http://192.168.4.1/" }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "timeout": "4s",
  "retryOn429": true,
  "retryCount": 2,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}